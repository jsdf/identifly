    "packagerOpts": {
      "assetExts": [
        "ttf"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.yourapp.identifly-os",
      "supportsTablet": true
    },
    "android": {
      "package": "com.yourapp.identifly-os"
    }
  }
